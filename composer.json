        "WebSockets",
        "Server",
        "Client",
        "WAMP",
        "WAMP2",
        "Autobahn",
        "Sockets"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Bonneau", "email": "matt@bonneau.net", "role": "Developer"
        },

        {
            "name": "David Dan", "email": "davidwdan@gmail.com", "role": "Developer"
        }
    ],
    "autoload": {
