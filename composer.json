{
    "name": "thruway/router",
    "description": "Thruway WAMP router core",
    "license": "MIT",
    "keywords": [
        "WebSockets",
        "Server",
        "Client",
        "WAMP",
        "WAMP2",
        "Autobahn",
        "Sockets"
    ],
    "authors": [
        {
            "name": "Matt Bonneau", "email": "matt@bonneau.net", "role": "Developer"
        },

        {
            "name": "David Dan", "email": "davidwdan@gmail.com", "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Thruway\\": "src/"
        }
    },
    "bin": [
        "bin/thruway"
    ],
    "require": {
        "php": ">=5.6",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0|^5.0",
        "voryx/thruway-common": "^1.0.5",
        "thruway/client": "^0.5.0",
        "react/event-loop": "^1.0 || ^0.5 || ^0.4.3",
        "react/promise": "^2.3.0",
        "thruway/ratchet-transport": "dev-feature/symfony-5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5.4",
        "thruway/pawl-transport": "^0.5.0",
        "thruway/raw-socket-transport": "dev-feature/symfony-5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:lugus/ratchet-transport.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:lugus/raw-socket-transport.git"
        }
    ],
    "config": {
        "preferred-install": {
            "thruway/ratchet-transport": "source",
            "thruway/raw-socket-transport": "source"
        }
    }
}
